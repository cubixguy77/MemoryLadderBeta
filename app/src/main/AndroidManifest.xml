<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="speednumbers.mastersofmemory.com.presentation">

    <application
        android:name="selectChallenge.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme"
        android:launchMode="singleTop">
        <activity
            android:name="selectChallenge.viewChallengeList.ChallengeListActivity"
            android:windowSoftInputMode="stateAlwaysHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="memorization.NumberMemoryActivity"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:parentActivityName="selectChallenge.viewChallengeList.ChallengeListActivity">
        </activity>

    </application>

</manifest>